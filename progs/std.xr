



flip = fun f x y -> f y x

id = fun x -> x

const = fun x y -> x

fib = fun n -> if n < 2 
                then n 
                else fib (n - 1) + fib (n - 2)

fac = fun n -> if n < 2 
                then 1 
                else n * fac (n - 1)

foundation = fun x -> [x]

peano = fun n -> if n == 0
                    then []
                    else foundation (peano (n - 1))


